<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomScratchView">
        <attr
            name="scratchDrawable"
            format="reference|color" />
        <attr
            name="scratchWidth"
            format="dimension" />
    </declare-styleable>

    <declare-styleable name="BlurLayout">
        <attr name="blurColor" format="color"/>
        <attr name="blurOpacity" format="float"/>
        <attr name="blurImage" format="integer"/>
        <attr name="blurRadius" format="integer"/>
    </declare-styleable>
</resources>